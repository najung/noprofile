
<%= form_for @post, :html => {:multipart => true} do |f| %>
  <%= f.label :제목 %>
  <%= f.text_field :title %><br />
  <%= f.label :내용 %>
  <%= f.text_field :content %><br />
    <div class="form-group">
         <input style="background-color : transparent;" type="file" name="pic" accept="image/*" >
    </div>
  <%= f.submit %>
<% end %>


